{
  "host": "0.0.0.0",
  "port": 8080,
  "publicDir": "public",
  "cache": {
    "enabled": true,
    "maxItems": 1000,
    "maxFileSize": 10485760,
    "browserCacheTTL": 3600
  },
  "compression": {
    "enabled": true
  },
  "cors": {
    "enabled": true,
    "allowOrigin": "*",
    "allowMethods": "GET, HEAD, OPTIONS",
    "allowHeaders": "Origin, X-Requested-With, Content-Type, Accept"
  }
}
